@model VetApp.Models.ConsumosVet

@{
    ViewData["Title"] = "Delete";
}

<h1>Eliminar</h1>

<h4>ConsumosVet</h4>
<hr />
<div>
    <h4>¿Está seguro de que desea eliminar esto?</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Observaciones)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Observaciones)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.CantVacunas)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CantVacunas)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Nit)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Nit)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.CodMascotaNavigation)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CodMascotaNavigation.CodMascota)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.CodVacunaNavigation)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CodVacunaNavigation.CodVacuna)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.IdServicioNavigation)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdServicioNavigation.IdServicio)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="CodMascota" />
        <input type="hidden" asp-for="IdServicio" />
        <input type="hidden" asp-for="IdConsumoVet" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index">Regresar a la Lista</a>
    </form>
</div>
