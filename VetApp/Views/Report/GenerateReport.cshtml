@model VetApp.Models.ConsumosVetCreateViewModel

@{
    ViewData["Title"] = "Generate Medic Report";
}

<h1>Generate Medic Report</h1>

<form asp-action="GenerateReport" method="post">
    <div class="form-group">
        <label asp-for="FechaInicio" class="control-label"></label>
        <input asp-for="FechaInicio" class="form-control" />
        <span asp-validation-for="FechaInicio" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="FechaFin" class="control-label"></label>
        <input asp-for="FechaFin" class="form-control" />
        <span asp-validation-for="FechaFin" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary">Generate Report</button>
</form>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
