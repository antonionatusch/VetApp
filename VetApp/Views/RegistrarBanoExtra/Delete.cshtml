@model VetApp.Models.RegistrarBanoExtraViewModel

@{
    ViewData["Title"] = "Delete Baño Extra";
}

<h1>Delete Baño Extra</h1>

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>RegistrarBanoExtraViewModel</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            ID del Hospedaje
        </dt>
        <dd class="col-sm-10">
            @Model.IdHospedaje
        </dd>
        <dt class="col-sm-2">
            Cantidad de Baños
        </dt>
        <dd class="col-sm-10">
            @Model.CantidadBanos
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="IdHospedaje" />
        <div class="form-group">
            <input type="submit" value="Delete" class="btn btn-danger" /> |
            <a asp-action="Index" class="btn btn-secondary">Back to List</a>
        </div>
    </form>
</div>
